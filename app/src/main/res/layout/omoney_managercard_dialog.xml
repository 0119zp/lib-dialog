<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
                android:id="@+id/animation_mancard_ll"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:background="#646C6C6C"
    >

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_alignParentBottom="true"
        android:orientation="vertical">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_margin="@dimen/common_32"
            android:background="@drawable/corner_bg"
            android:orientation="vertical">

            <LinearLayout
                android:id="@+id/liner_mantitle"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="vertical"
                android:visibility="gone">

                <TextView
                    android:id="@+id/animation_mantitle"
                    android:layout_width="match_parent"
                    android:layout_height="50dip"
                    android:gravity="center"
                    android:minHeight="50dp"
                    android:padding="5dp"
                    android:text=""
                    android:textColor="#ffcccccc"
                    android:textSize="10dip"/>
            </LinearLayout>

            <LinearLayout
                android:id="@+id/liner_manone"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="vertical"
                android:visibility="gone">

                <TextView
                    android:id="@+id/animation_manone"
                    android:layout_width="match_parent"
                    android:layout_height="40dip"
                    android:gravity="center"
                    android:minHeight="50dp"
                    android:padding="10dp"
                    android:textColor="#4BA6D1"/>
            </LinearLayout>

            <LinearLayout
                android:id="@+id/liner_mantwo"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="vertical"
                android:visibility="gone">

                <View
                    style="@style/setting_ico_view_style"
                    android:paddingLeft="5dp"
                    android:paddingRight="5dp"
                    />

                <TextView
                    android:id="@+id/animation_mantwo"
                    android:layout_width="match_parent"
                    android:layout_height="40dip"
                    android:gravity="center"
                    android:minHeight="50dp"
                    android:padding="10dp"
                    android:text=""

                    android:textColor="#4BA6D1"/>
            </LinearLayout>

            <LinearLayout
                android:id="@+id/liner_manthree"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="vertical"
                android:visibility="gone">

                <View
                    style="@style/setting_ico_view_style"
                    android:paddingLeft="5dp"
                    android:paddingRight="5dp"
                    />

                <TextView
                    android:id="@+id/animation_manthree"
                    android:layout_width="match_parent"
                    android:layout_height="40dip"
                    android:clickable="true"
                    android:gravity="center"
                    android:text=""
                    android:textColor="#4BA6D1"
                    android:textSize="14sp"/>

            </LinearLayout>

            <LinearLayout
                android:id="@+id/liner_manfour"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="vertical"
                android:visibility="gone">

                <View
                    style="@style/setting_ico_view_style"
                    android:paddingLeft="5dp"
                    android:paddingRight="5dp"
                    />

                <TextView
                    android:id="@+id/animation_manfour"
                    android:layout_width="match_parent"
                    android:layout_height="40dip"
                    android:clickable="true"
                    android:gravity="center"
                    android:text=""
                    android:textColor="#4BA6D1"
                    android:textSize="14sp"/>

            </LinearLayout>

            <LinearLayout
                android:id="@+id/liner_manfive"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="vertical"
                android:visibility="gone">

                <View
                    style="@style/setting_ico_view_style"
                    android:paddingLeft="5dp"
                    android:paddingRight="5dp"
                    />

                <TextView
                    android:id="@+id/animation_manfive"
                    android:layout_width="match_parent"
                    android:layout_height="@dimen/common_80"
                    android:clickable="true"
                    android:gravity="center"
                    android:text=""
                    android:textColor="#4BA6D1"
                    android:textSize="@dimen/common_32"/>

            </LinearLayout>

            <LinearLayout
                android:id="@+id/liner_mansix"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="vertical"
                android:visibility="gone">

                <View
                    style="@style/setting_ico_view_style"
                    android:paddingLeft="5dp"
                    android:paddingRight="5dp"
                    />

                <TextView
                    android:id="@+id/animation_mansix"
                    android:layout_width="match_parent"
                    android:layout_height="@dimen/common_80"
                    android:clickable="true"
                    android:gravity="center"
                    android:text=""
                    android:textColor="#4BA6D1"
                    android:textSize="@dimen/common_32"/>

            </LinearLayout>

            <LinearLayout
                android:id="@+id/liner_manseven"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="vertical"
                android:visibility="gone">

                <View
                    style="@style/setting_ico_view_style"
                    android:paddingLeft="5dp"
                    android:paddingRight="5dp"
                    />

                <TextView
                    android:id="@+id/animation_manseven"
                    android:layout_width="match_parent"
                    android:layout_height="@dimen/common_80"
                    android:clickable="true"
                    android:gravity="center"
                    android:text=""
                    android:textColor="#4BA6D1"
                    android:textSize="@dimen/common_32"
                    android:visibility="gone"/>

            </LinearLayout>

            <LinearLayout
                android:id="@+id/liner_maneight"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="vertical"
                android:visibility="gone">

                <View
                    style="@style/setting_ico_view_style"
                    android:paddingLeft="5dp"
                    android:paddingRight="5dp"
                    />

                <TextView
                    android:id="@+id/animation_maneight"
                    android:layout_width="match_parent"
                    android:layout_height="@dimen/common_80"
                    android:clickable="true"
                    android:gravity="center"
                    android:text=""
                    android:textColor="#4BA6D1"
                    android:textSize="@dimen/common_32"
                    />
            </LinearLayout>
        </LinearLayout>

        <TextView
            android:id="@+id/animation_mancard_cancel"
            android:layout_width="match_parent"
            android:layout_height="@dimen/common_80"
            android:layout_marginBottom="@dimen/common_30"
            android:layout_marginLeft="@dimen/common_30"
            android:layout_marginRight="@dimen/common_30"
            android:background="@drawable/corner_bg"
            android:gravity="center"
            android:text="取消"
            android:textColor="#2072cf"
            android:textSize="@dimen/common_32"/>
    </LinearLayout>

</RelativeLayout>